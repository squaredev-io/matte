{
  "name": "matte-ui",
  "version": "0.11.1",
  "description": "Matte is a UI component library on top of MUI and other react libraries.",
  "author": "Squaredev",
  "license": "Apache-2.0",
  "repository": "https://github.com/squaredev-io/matte",
  "module": "./es6/index.js",
  "exports": {
    ".": {
      "import": "./es6/index.js",
      "require": "./cjs/index.js"
    }
  },
  "types": "./types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf ./dist && rollup   --config rollup.config.ts",
    "start": "rollup -c -w",
    "storybook": "start-storybook -s ./ -p 9090",
    "predeploy-storybook": "build-storybook && npm run copy-storybook-assets",
    "deploy-storybook": "storybook-to-ghpages -e storybook-static",
    "copy-storybook-assets": "copyfiles images/* storybook-static",
    "prepublish": "npm run build && cp package.json dist && cp README.md dist && cd dist",
    "publish-beta": "npm publish --tag beta"
  },
  "dependencies": {
    "@mui/base": "5.0.0-alpha.67",
    "@mui/lab": "^5.0.0-alpha.72",
    "@nivo/bar": "^0.74.1",
    "@nivo/core": "0.74.1",
    "@nivo/line": "0.74.1",
    "@nivo/pie": "^0.74.1",
    "@nivo/tooltip": "^0.74.1",
    "mathjs": "^10.0.0",
    "react-copy-to-clipboard": "^5.0.3",
    "react-feather": "^2.0.9"
  },
  "peerDependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.0",
    "@mui/styles": "^5.2.0",
    "material-ui-popup-state": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@storybook/addon-actions": "^6.4.1",
    "@storybook/addon-docs": "^6.4.1",
    "@storybook/addon-essentials": "^6.4.1",
    "@storybook/addon-links": "^6.4.1",
    "@storybook/builder-webpack5": "6.4.9",
    "@storybook/manager-webpack5": "6.4.9",
    "@storybook/react": "^6.4.1",
    "@storybook/storybook-deployer": "2.8.10",
    "@types/mathjs": "^6.0.8",
    "@types/react": "^17.0.37",
    "@types/react-copy-to-clipboard": "5.0.0",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "css-loader": "5.2.7",
    "html-webpack-plugin": "^5.5.0",
    "postcss": "^8.4.4",
    "rollup": "^2.60.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "sass": "1.44.0",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.2.2",
    "webpack-dev-server": "^4.6.0"
  }
}
